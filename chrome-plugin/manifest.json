{
    "manifest_version": 2,

    "name": "TellMe.Cat Chrome Plugin",
    "description": "Add websites to TellMe.Cat via Chrome",
    "version": "1.0",

    "permissions": [
        "127.0.0.1",
        "http://tellme.cat/"
        ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}
